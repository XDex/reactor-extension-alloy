{
  "name": "reactor-extension-alloy",
  "version": "0.0.1",
  "description": "The Adobe Experience Platform Web SDK allows for streaming data into the platform, syncing identities, personalizing content, and more.",
  "author": "Adobe",
  "license": "Apache-2.0",
  "scripts": {
    "build:dev": "parcel src/view/actions/sendEvent.html --public-url ./",
    "build:prod": "parcel build src/view/actions/sendEvent.html --public-url ./",
    "package": "npm run build:prod && npx @adobe/reactor-packager",
    "sandbox": "npx @adobe/reactor-sandbox",
    "dev": "npm-run-all --parallel build:dev sandbox",
    "lint": "eslint \"src/**/*.jsx\""
  },
  "dependencies": {
    "@react/react-spectrum": "^2.20.0",
    "classnames": "^2.2.6",
    "formik": "^1.5.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "yup": "^0.27.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "stylus": "^0.54.5"
  }
}
